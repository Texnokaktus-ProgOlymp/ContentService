{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "{Timestamp:HH:mm:ss.fff K} [{Level:u3}] <{SourceContext}> {Message:lj} {NewLine}{Exception}"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithThreadId",
            "WithExceptionDetails",
            "WithEnvironmentName",
            "WithAssemblyName",
            "WithMachineName"
        ]
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultDb": "data source=raspberrypi.local;initial catalog=ContentService;User Id=sa;Password=Raspberry-Pi;Encrypt=True;TrustServerCertificate=True",
        "DefaultRedis": "raspberrypi.local",
        "OtlpReceiver": "http://raspberrypi.local:4317"
    },
    "S3": {
        "ServiceURL": "https://s3.progolymp.cttit.ru",
        "ForcePathStyle": true
    }
}
